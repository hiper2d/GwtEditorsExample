<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="res" type="com.hiper2d.client.resourcebundle.Resources"/>
    <ui:with field="msg" type="com.hiper2d.client.message.Messages"/>

    <g:DockLayoutPanel ui:field="dockPanel" unit="PX">
        <g:west size="200">
            <g:SimplePanel ui:field="westContainer" addStyleNames="{res.basic.westPanel}">
                <g:FlowPanel addStyleNames="{res.basic.verticalPanel}">
                    <g:Button ui:field="employeeButton" text="Employee"/>
                    <g:Button ui:field="phoneBookButton" text="Book"/>
                    <g:Button ui:field="carButton" text="Car"/>
                </g:FlowPanel>
            </g:SimplePanel>
        </g:west>
        <g:north size="100">
            <g:SimplePanel ui:field="northContainer" addStyleNames="{res.basic.northPanel}"></g:SimplePanel>
        </g:north>
        <g:center>
            <g:SimplePanel ui:field="centerContainer" addStyleNames="{res.basic.centerPanel}"></g:SimplePanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>