<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="msg" type="com.hiper2d.client.message.Messages"/>
    <ui:with field="res" type="com.hiper2d.client.resourcebundle.Resources"/>

    <g:HTMLPanel>

        <g:FlowPanel addStyleNames="{res.basic.employeeEditorWraper}">
            <g:FlowPanel>
                <g:Label addStyleNames="{res.basic.employeeLabel}" text="{msg.labelPhone}"/>
                <g:Label ui:field="id"/>
            </g:FlowPanel>
            <g:FlowPanel>
                <g:Label addStyleNames="{res.basic.employeeLabel}" text="{msg.labelPhone}"/>
                <g:TextBox addStyleNames="{res.basic.employeeNameText}" ui:field="name" visibleLength="30" maxLength="30"></g:TextBox>
            </g:FlowPanel>
        </g:FlowPanel>
    </g:HTMLPanel>
</ui:UiBinder>